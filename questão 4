int subtrairComplemento2(int n, int A[], int B[], int C[]) {
    int x = 0;
    for (int i = n - 1; i >= 0; i--) {
        int sub = A[i] - B[i] - x;
        if (sub < 0) { C[i] = sub + 2; x = 1; }
        else { C[i] = sub; x = 0; }
    }
    if (A[0] != B[0] && C[0] != A[0]) return -1; 
    return 0;
}
